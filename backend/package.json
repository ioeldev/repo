{
    "name": "backend",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "build": "rm -rf .build && tsc && tsc-alias && pnpm run postbuild",
        "postbuild": "copyfiles -a \"src/**/*\" \"*.json\" dist -e \"**/*.js\" -e \"**/*.ts\"",
        "start": "node dist/src/index.js",
        "dev": "NODE_ENV=local nodemon src/index.ts",
        "start:liquidation": "node dist/src/liquidation-server.js",
        "dev:liquidation": "NODE_ENV=local nodemon src/liquidation-server.ts",
        "deploy:capi": "serverless deploy --stage capi",
        "deploy:capi-old": "serverless deploy --stage capi-old",
        "deploy:inter": "serverless deploy --stage inter"
    },
    "dependencies": {
        "@types/express": "^4.17.21",
        "@types/moment": "^2.13.0",
        "@types/node": "^20.12.6",
        "axios": "^1.7.2",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-fileupload": "^1.5.2",
        "jsdom": "^24.1.0",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.30.1",
        "moment-timezone": "^0.5.45",
        "mongodb": "^6.5.0",
        "nodemailer": "^6.9.15",
        "serverless-http": "^3.2.0",
        "ws": "^8.16.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express-fileupload": "^1.5.1",
        "@types/jsdom": "^21.1.7",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/nodemailer": "^6.4.16",
        "@types/ws": "^8.5.10",
        "copyfiles": "^2.4.1",
        "nodemon": "^3.1.3",
        "serverless": "3",
        "serverless-dotenv-plugin": "^6.0.0",
        "serverless-plugin-typescript": "^2.1.5",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.4.5"
    },
    "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
